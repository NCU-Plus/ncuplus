{
  "name": "backend",
  "private": true,
  "version": "1.3.17",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:college": "node ./dist/seeder.js college",
    "seed:college:clear": "node ./dist/seeder.js college:clear",
    "seed:course": "node ./dist/seeder.js course",
    "seed:course:clear": "node ./dist/seeder.js course:clear",
    "seed:department": "node ./dist/seeder.js department",
    "seed:department:clear": "node ./dist/seeder.js department:clear"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express-session": "^1.17.3",
    "file-type": "^16.5.4",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.5",
    "nest-casl": "^1.9.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/cookie-parser": "^1.4.6",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.8",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-oauth2": "^1.4.15",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
